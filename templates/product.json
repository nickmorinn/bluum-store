        "liquid_btFKek": {
          "type": "liquid",
          "name": "Price Bundle [HTML + JS]",
          "settings": {
            "liquid": "<div class=\"zs-offer-wrappers\">\n  <span>Price:</span>\n  <div class=\"zs-offer-wrapper\">\n    <div class=\"zs-offer-box zs-active\" data-qty=\"1\">\n      <div class=\"zs-radio\"><span></span></div>\n      <div class=\"zs-title\">Buy 1</div>\n      <div class=\"zs-sub\">Standard</div>\n      <div class=\"zs-price-default\">--</div>\n    </div>\n    <div class=\"zs-offer-box\" data-qty=\"2\">\n      <div class=\"zs-radio\"><span></span></div>\n      <div class=\"zs-title\">Buy 2</div>\n      <div class=\"zs-sub zs-green\">Save 5%</div>\n      <div class=\"zs-price\">--</div>\n      <div class=\"zs-old-price\">--</div>\n    </div>\n    <div class=\"zs-offer-box\" data-qty=\"3\">\n      <div class=\"zs-radio\"><span></span></div>\n      <div class=\"zs-title\">Buy 3 +</div>\n      <div class=\"zs-sub zs-green\">Save 10%</div>\n      <div class=\"zs-price\">--</div>\n      <div class=\"zs-old-price\">--</div>\n    </div>\n  </div>\n</div>\n\n<script>\n(function() {\n  'use strict';\n  const LOG_PREFIX = '[ZS-KLAVIYO-PARSER]';\n\n  function getUnitPriceFromKlaviyo() {\n    try {\n      // 1. Target the specific script tag you found\n      const scriptTag = document.getElementById('viewed_product');\n      if (!scriptTag) {\n        console.error(`${LOG_PREFIX} Script tag #viewed_product not found.`);\n        return null;\n      }\n\n      const content = scriptTag.textContent;\n\n      // 2. Look for Value: \"40.00\" using Regex\n      // This pattern looks for the word Value, then a colon, then a number inside quotes\n      const regex = /Value:\\s*\"([^\"]+)\"/;\n      const match = content.match(regex);\n\n      if (match && match[1]) {\n        const price = parseFloat(match[1]);\n        if (!isNaN(price)) {\n          console.log(`${LOG_PREFIX} Successfully extracted unit price:`, price);\n          return price;\n        }\n      }\n      \n      // 3. If \"Value\" isn't found, try \"Price\"\n      const priceRegex = /Price:\\s*\"\\$([^\"]+)\"/;\n      const priceMatch = content.match(priceRegex);\n      if (priceMatch && priceMatch[1]) {\n         return parseFloat(priceMatch[1]);\n      }\n\n    } catch (e) {\n      console.error(`${LOG_PREFIX} Error parsing script tag:`, e);\n    }\n    return null;\n  }\n\n  function render() {\n    const unitPrice = getUnitPriceFromKlaviyo();\n    \n    if (!unitPrice) {\n      // Fallback: If Klaviyo script isn't ready, try to get from standard meta\n      // But only if it's a small number (to avoid the cart total bug)\n      console.log(`${LOG_PREFIX} Klaviyo price not found, checking fallback...`);\n      return;\n    }\n\n    const boxes = document.querySelectorAll('.zs-offer-box');\n    const mainQtyInput = document.querySelector('input[name=\"quantity\"]');\n\n    boxes.forEach(box => {\n      const baseQty = parseInt(box.dataset.qty);\n      let calcQty = baseQty;\n\n      // Logic for Buy 3+ scaling\n      if (baseQty === 3 && mainQtyInput && parseInt(mainQtyInput.value) >= 3) {\n        calcQty = parseInt(mainQtyInput.value);\n      }\n\n      const discount = calcQty >= 3 ? 0.10 : (calcQty >= 2 ? 0.05 : 0);\n      const originalTotal = unitPrice * calcQty;\n      const discountedTotal = originalTotal * (1 - discount);\n\n      if (baseQty === 1) {\n        const p = box.querySelector('.zs-price-default');\n        if (p) p.textContent = '$' + unitPrice.toFixed(2);\n      } else {\n        const s = box.querySelector('.zs-price');\n        const o = box.querySelector('.zs-old-price');\n        if (s) s.textContent = '$' + discountedTotal.toFixed(2);\n        if (o) o.textContent = '$' + originalTotal.toFixed(2);\n      }\n    });\n  }\n\n  function init() {\n    // We don't need a mutation observer here because this script tag \n    // is static and doesn't change when the cart updates.\n    \n    // Handle variant selection (if the page reloads or script updates)\n    document.addEventListener('change', (e) => {\n      if (e.target.name === 'id') {\n        // Many themes reload the page or update the price area. \n        // We wait a bit then re-render.\n        setTimeout(render, 300);\n      }\n    });\n\n    // Bundle click logic\n    document.querySelectorAll('.zs-offer-box').forEach(box => {\n      box.addEventListener('click', function() {\n        document.querySelectorAll('.zs-offer-box').forEach(b => b.classList.remove('zs-active'));\n        this.classList.add('zs-active');\n        \n        const qInput = document.querySelector('input[name=\"quantity\"]');\n        if (qInput) {\n          qInput.value = this.dataset.qty;\n          qInput.dispatchEvent(new Event('change', { bubbles: true }));\n        }\n        render();\n      });\n    });\n\n    // Run on load\n    render();\n    // Safety delay\n    setTimeout(render, 1000);\n  }\n\n  if (document.readyState === 'loading') {\n    document.addEventListener('DOMContentLoaded', init);\n  } else {\n    init();\n  }\n})();\n</script>\n\n<style>\n  .zs-offer-wrapper { display: flex; gap: 10px; margin: 15px 0; }\n  .zs-offer-box { border: 1px solid #ddd; padding: 15px; flex: 1; text-align: center; cursor: pointer; border-radius: 8px; }\n  .zs-offer-box.zs-active { border: 2px solid #000; background: #fafafa; }\n  .zs-price, .zs-price-default { font-weight: bold; display: block; margin-top: 5px; font-size: 1.1em; }\n  .zs-old-price { text-decoration: line-through; color: #999; font-size: 0.85em; }\n  .zs-green { color: #2d8a4e; font-weight: bold; font-size: 0.85em; }\n</style>"
