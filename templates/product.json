r) {\n      elements.quantitySelector.style.display = 'none';\n    }\n    \n    const clonedSelector = document.querySelector('.zs-cloned-quantity');\n    if (clonedSelector) {\n      clonedSelector.style.display = 'none';\n    }\n    \n  }\n\n  function enableAllBundles() {\n    elements.offerBoxes.forEach(box => {\n      box.style.opacity = '1';\n      box.style.pointerEvents = 'auto';\n      box.classList.remove('zs-disabled');\n    });\n    \n    const firstBox = document.querySelector('.zs-offer-box[data-qty=\"1\"]');\n    if (firstBox && !document.querySelector('.zs-offer-box.zs-active')) {\n      firstBox.classList.add('zs-active');\n    }\n    \n    hideInventoryNotice();\n  }\n\n  function updateButtonStates(qtyField) {\n    if (!qtyField) return;\n    \n    const currentQty = parseInt(qtyField.value);\n    const minQty = parseInt(qtyField.getAttribute('min')) || 1;\n    const quantitySelector = qtyField.closest('quantity-selector') || qtyField.closest('.product-info__quantity-selector');\n    \n    if (!quantitySelector) return;\n    \n    const buttons = quantitySelector.querySelectorAll(SELECTORS.qtyButton);\n    if (buttons.length < 2) return;\n    \n    const minusButton = buttons[0];\n    const plusButton = buttons[1];\n\n    if (currentQty <= minQty) {\n      minusButton.style.opacity = '0.3';\n      minusButton.style.cursor = 'not-allowed';\n      minusButton.style.pointerEvents = 'none';\n      minusButton.classList.add('disabled');\n    } else {\n      minusButton.style.opacity = '1';\n      minusButton.style.cursor = 'pointer';\n      minusButton.style.pointerEvents = 'auto';\n      minusButton.classList.remove('disabled');\n    }\n\n    if (maxInventory !== null && currentQty >= maxInventory) {\n      plusButton.style.opacity = '0.3';\n      plusButton.style.cursor = 'not-allowed';\n      plusButton.style.pointerEvents = 'none';\n      plusButton.classList.add('disabled');\n      showInventoryNotice(`Only ${maxInventory} item${maxInventory !== 1