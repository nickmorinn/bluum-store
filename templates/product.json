ariantData.available;\n      return available === true;\n    }\n\n    // Fallback: Check from variant select option\n    const variantSelect = document.querySelector('select[name=\"id\"]');\n    if (variantSelect) {\n      const selectedOption = variantSelect.options[variantSelect.selectedIndex];\n      if (selectedOption) {\n        const available = selectedOption.dataset.available;\n        if (available !== undefined) {\n          return available === 'true';\n        }\n      }\n    }\n\n    // Fallback: Check from Add to Cart button state\n    const addToCartButton = document.querySelector('button[name=\"add\"]') || \n                           document.querySelector('[data-add-to-cart]') ||\n                           document.querySelector('.product-form__submit');\n    if (addToCartButton) {\n      const isDisabled = addToCartButton.disabled || \n                        addToCartButton.classList.contains('disabled') ||\n                        addToCartButton.hasAttribute('disabled');\n      \n      const buttonText = addToCartButton.textContent.toLowerCase();\n      const isSoldOut = buttonText.includes('sold out') || \n                       buttonText.includes('unavailable') ||\n                       buttonText.includes('out of stock');\n      \n      if (isDisabled || isSoldOut) {\n        return false;\n      }\n    }\n\n    return true;\n  }\n\n  async function getMaxInventory() {\n    const variantId = getCurrentVariantId();\n    if (!variantId) return null;\n\n    // First try to fetch from product.js\n    const variantData = await fetchVariantAvailability(variantId);\n    if (variantData && variantData.inventory_quantity !== undefined) {\n      return variantData.inventory_quantity;\n    }\n\n    // Fallback to meta object\n    if (typeof meta !== 'undefined' && meta.product?.variants) {\n      const variant = meta.product.variants.find(v => String(v.id) === String(variantId));\n      if (variant) {\n        \n        setTimeout(() => {\n         