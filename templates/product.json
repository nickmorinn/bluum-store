          "name": "Price Bundle [HTML + JS]",
          "settings": {
            "liquid": "<div class=\"zs-offer-wrappers\">\n  <span>Price:</span>\n  <div class=\"zs-offer-wrapper\">\n    <div class=\"zs-offer-box zs-active\" data-qty=\"1\">\n      <div class=\"zs-radio\"><span></span></div>\n      <div class=\"zs-title\">Buy 1</div>\n      <div class=\"zs-sub\">Standard</div>\n      <div class=\"zs-price-default\">--</div>\n    </div>\n    <div class=\"zs-offer-box\" data-qty=\"2\">\n      <div class=\"zs-radio\"><span></span></div>\n      <div class=\"zs-title\">Buy 2</div>\n      <div class=\"zs-sub zs-green\">Save 5%</div>\n      <div class=\"zs-price\">--</div>\n      <div class=\"zs-old-price\">--</div>\n    </div>\n    <div class=\"zs-offer-box\" data-qty=\"3\">\n      <div class=\"zs-radio\"><span></span></div>\n      <div class=\"zs-title\">Buy 3 +</div>\n      <div class=\"zs-sub zs-green\">Save 10%</div>\n      <div class=\"zs-price\">--</div>\n      <div class=\"zs-old-price\">--</div>\n    </div>\n  </div>\n  <div class=\"zs-inventory-notice\" style=\"display:none; color: #856404; background-color: #fff3cd; border: 1px solid #ffeeba; padding: 8px; border-radius: 4px; margin-top: 10px; font-size: 13px; text-align: center;\"></div>\n</div>\n\n<script>\n(function() {\n  'use strict';\n  \n  const SELECTORS = {\n    mainQtyField: 'input[name=\"quantity\"]',\n    qtyButton: '.quantity-selector__button, .quantity__button, .qty-btn, .quantity-button',\n    offerBox: '.zs-offer-box',\n    qtySelectorArea: '.product-info__quantity-selector, quantity-selector, .product-form__input--quantity',\n    variantInput: 'input[name=\"id\"], select[name=\"id\"]'\n  };\n\n  let maxStock = 999;\n  let unitPrice = 0;\n\n  // 1. GET CLEAN PATH (Ignores UTMs and Params)\n  function getBaseProductUrl() {\n    // window.location.pathname is always \"/products/name\" \n    // It never includes ?utm_source=... or &variant=...\n    return window.location.pathname;\n  }\n\n  // 2. FETCH UNIT PRICE (From Merchandise JSON)\n  async function fetchProductData() {\n    try {\n      const response = await fetch(getBaseProductUrl() + '.js');\n      const product = await response.json();\n      const currentId = document.querySelector(SELECTORS.variantInput)?.value;\n      const variant = product.variants.find(v => String(v.id) === String(currentId));\n      \n      if (variant) {\n        unitPrice = variant.price / 100;\n        console.log('[ZS-DEBUG] Base Merchandise Price:', unitPrice);\n      }\n    } catch (e) { console.error('[ZS] Price Fetch Error', e); }\n  }\n\n  // 3. DETECT ACTUAL STOCK (Cart Probe)\n  async function detectStockLimit() {\n    const variantId = document.querySelector(SELECTORS.variantInput)?.value;\n    if (!variantId) return;\n\n    try {\n      const response = await fetch('/cart/add.js', {\n        method: 'POST',\n        headers: { 'Content-Type': 'application/json', 'X-Requested-With': 'XMLHttpRequest' },\n        body: JSON.stringify({ items: [{ id: variantId, quantity: 999 }] })\n      });\n      const data = await response.json();\n\n      if (data.status === 422) {\n        // Shopify returns error like \"You can only add 17 of this item\"\n        const match = data.description.match(/\\d+/g);\n        if (match) {\n          maxStock = parseInt(match[match.length - 1]);\n          console.log('[ZS-DEBUG] Stock Limit Detected:', maxStock);\n        }\n      } else {\n        // If 999 was allowed, stock tracking is off\n        maxStock = 999;\n      }\n    } catch (e) { maxStock = 999; }\n  }\n\n  // 4. RENDERING & BUTTON LOGIC\n  function render() {\n    if (!unitPrice) return;\n\n    document.querySelectorAll(SELECTORS.offerBox).forEach(box => {\n      const baseQty = parseInt(box.dataset.qty);\n      const isActive = box.classList.contains('zs-active');\n      let calcQty = baseQty;\n\n      if (baseQty === 3 && isActive) {\n        const input = box.querySelector('input') || document.querySelector(SELECTORS.mainQtyField);\n        calcQty = parseInt(input.value) || 3;\n        box.querySelector('.zs-title').textContent = `Buy ${calcQty}`;\n      }\n\n      const discount = calcQty >= 3 ? 0.10 : (calcQty >= 2 ? 0.05 : 0);\n      const originalTotal = unitPrice * calcQty;\n      const discountedTotal = originalTotal * (1 - discount);\n\n      if (baseQty === 1) {\n        box.querySelector('.zs-price-default').textContent = '$' + unitPrice.toFixed(2);\n      } else {\n        box.querySelector('.zs-price').textContent = '$' + discountedTotal.toFixed(2);\n        box.querySelector('.zs-old-price').textContent = '$' + originalTotal.toFixed(2);\n      }\n      \n      updateButtonStates(box, calcQty);\n    });\n  }\n\n  function updateButtonStates(box, currentQty) {\n    const plusBtn = box.querySelector(`${SELECTORS.qtyButton}:last-child`) || \n                    document.querySelectorAll(SELECTORS.qtyButton)[1];\n    \n    if (plusBtn) {\n      if (currentQty >= maxStock) {\n        plusBtn.style.opacity = '0.3';\n        plusBtn.style.pointerEvents = 'none';\n        plusBtn.setAttribute('disabled', 'disabled');\n      } else {\n        plusBtn.style.opacity = '1';\n        plusBtn.style.pointerEvents = 'auto';\n        plusBtn.removeAttribute('disabled');\n      }\n    }\n  }\n\n  function syncQty(val) {\n    const main = document.querySelector(SELECTORS.mainQtyField);\n    if (main) {\n      main.value = val;\n      main.dispatchEvent(new Event('change', { bubbles: true }));\n    }\n    render();\n  }\n\n  function handleClones(qty) {\n    const originalArea = document.querySelector(SELECTORS.qtySelectorArea);\n    if (!originalArea) return;\n\n    if (qty === '3') {\n      const box3 = document.querySelector('.zs-offer-box[data-qty=\"3\"]');\n      let cloned = box3.querySelector('.zs-cloned-quantity');\n      if (!cloned) {\n        cloned = originalArea.cloneNode(true);\n        cloned.classList.add('zs-cloned-quantity');\n        box3.appendChild(cloned);\n        const input = cloned.querySelector('input');\n        input.value = '3';\n        input.addEventListener('input', (e) => {\n          let v = parseInt(e.target.value) || 3;\n          if (v < 3) v = 3;\n          if (v > maxStock) v = maxStock;\n          e.target.value = v;\n          syncQty(v);\n        });\n      }\n      originalArea.style.display = 'none';\n    } else {\n      originalArea.style.display = 'block';\n      const cloned = document.querySelector('.zs-cloned-quantity');\n      if (cloned) cloned.remove();\n      document.querySelector('.zs-offer-box[data-qty=\"3\"] .zs-title').textContent = \"Buy 3 +\";\n    }\n  }\n\n  async function init() {\n    await fetchProductData();\n    await detectStockLimit();\n    render();\n\n    document.querySelectorAll(SELECTORS.offerBox).forEach(box => {\n      box.addEventListener('click', function() {\n        document.querySelectorAll(SELECTORS.offerBox).forEach(b => b.classList.remove('zs-active'));\n        this.classList.add('zs-active');\n        handleClones(this.dataset.qty);\n        syncQty(this.dataset.qty);\n      });\n    });\n\n    document.addEventListener('click', (e) => {\n      const btn = e.target.closest(SELECTORS.qtyButton);\n      if (!btn || !btn.closest('.zs-cloned-quantity')) return;\n      e.preventDefault();\n\n      const input = btn.closest('.zs-cloned-quantity').querySelector('input');\n      let val = parseInt(input.value) || 3;\n      \n      const isMinus = btn.textContent.includes('-') || btn.classList.contains('minus') || btn === btn.parentNode.firstElementChild;\n\n      if (isMinus) {\n        if (val > 3) val--;\n      } else {\n        if (val < maxStock) val++;\n      }\n\n      input.value = val;\n      syncQty(val);\n    }, true);\n\n    // Watch for variant switching (re-triggers on variant ID change)\n    document.addEventListener('change', async (e) => {\n      if (e.target.name === 'id') {\n        await fetchProductData();\n        await detectStockLimit();\n        render();\n      }\n    });\n  }\n\n  window.addEventListener('load', init);\n})();\n</script>\n\n<style>\n  .zs-offer-wrapper { display: flex; gap: 10px; margin: 15px 0; flex-wrap: wrap; }\n  .zs-offer-box { border: 1px solid #ddd; padding: 15px; flex: 1; text-align: center; cursor: pointer; border-radius: 8px; position: relative; min-width: 140px; }\n  .zs-offer-box.zs-active { border: 2px solid #000; background: #fafafa; }\n  .zs-price, .zs-price-default { font-weight: bold; display: block; margin-top: 5px; font-size: 1.1em; }\n  .zs-old-price { text-decoration: line-through; color: #999; font-size: 0.85em; }\n  .zs-green { color: #2d8a4e; font-weight: bold; font-size: 0.85em; }\n  .zs-cloned-quantity { margin-top: 10px; }\n  .zs-popular { position: absolute; top: -12px; left: 50%; transform: translateX(-50%); background: #000; color: #fff; padding: 2px 10px; font-size: 10px; border-radius: 10px; }\n</style>"
