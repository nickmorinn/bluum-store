lector('.zs-offer-box[data-qty=\"3\"]');\n    if (maxInventory !== null && maxInventory < 3 && buy3Box) {\n      buy3Box.classList.add('zs-disabled');\n      buy3Box.style.opacity = '0.5';\n    }\n  }\n\n  function cacheElements() {\n    elements = {\n      zsOffer: document.querySelector(SELECTORS.zsOffer),\n      offerBoxes: document.querySelectorAll(SELECTORS.offerBox),\n      qtyField: document.querySelector(SELECTORS.qtyField),\n      inventoryNotice: document.querySelector(SELECTORS.inventoryNotice),\n      variantPicker: document.querySelector(SELECTORS.variantPicker)\n    };\n  }\n\n  // --- 5. INITIALIZATION ---\n  async function init() {\n    cacheElements();\n    await initInventory();\n    \n    const initialBox = document.querySelector('.zs-offer-box.zs-active');\n    if (initialBox) renderPrices();\n\n    // Bundle Clicks\n    elements.offerBoxes.forEach(box => {\n      box.addEventListener('click', function() {\n        if (this.classList.contains('zs-disabled')) return;\n        elements.offerBoxes.forEach(b => b.classList.remove('zs-active'));\n        this.classList.add('zs-active');\n        \n        if (elements.qtyField) {\n          elements.qtyField.value = this.dataset.qty;\n          elements.qtyField.dispatchEvent(new Event('change', { bubbles: true }));\n        }\n        handleQuantityBox(this.dataset.qty);\n        renderPrices();\n      });\n    });\n\n    // Variant Change\n    document.addEventListener('change', async (e) => {\n      if (e.target.name === 'id') {\n        await initInventory();\n        renderPrices();\n      }\n    });\n\n    // Handle Cloned Quantity Buttons (+ / -)\n    document.addEventListener('click', (e) => {\n      const btn = e.target.closest(SELECTORS.qtyButton);\n      if (!btn) return;\n      \n      const clonedParent = btn.closest('.zs-cloned-quantity');\n      if (!clonedParent) return;\n\n      e.preventDefault();\n      const input = clonedParent.querySelector('input[type=\"number\"]');\n      let val = parseInt(input.value);\n      \n      // Determine if Plus or Minus (usually minus is the first button)\n      const isMinus = btn.previousElementSibling === null || btn.classList.contains('minus') || btn.textContent.includes('-');\n      \n      if (isMinus) {\n        if (val > 3) val--;\n      } else {\n        if (!maxInventory || val < maxInventory) val++;\n        else showInventoryNotice(`Only ${maxInventory} available`, 'warning');\n      }\n\n      input.value = val;\n      if (elements.qtyField) {\n        elements.qtyField.value = val;\n        elements.qtyField.dispatchEvent(new Event('change', { bubbles: true }));\n      }\n      renderPrices();\n    }, true);\n  }\n\n  if (document.readyState === 'loading') document.addEventListener('DOMContentLoaded', init);\n  else init();\n\n})();\n</script>\n\n<style>\n  .zs-offer-wrapper { display: flex; gap: 10px; margin: 15px 0; flex-wrap: wrap; }\n  .zs-offer-box { border: 1px solid #ddd; padding: 15px; flex: 1; text-align: center; cursor: pointer; border-radius: 8px; position: relative; min-width: 130px; }\n  .zs-offer-box.zs-active { border: 2px solid #000; background: #fafafa; }\n  .zs-price, .zs-price-default { font-weight: bold; display: block; margin-top: 5px; font-size: 1.1em; }\n  .zs-old-price { text-decoration: line-through; color: #999; font-size: 0.85em; }\n  .zs-green { color: #2d8a4e; font-weight: bold; font-size: 0.85em; }\n  .zs-cloned-quantity { margin-top: 10px; display: none; }\n  .zs-disabled { cursor: not-allowed; }\n  .zs-popular { position: absolute; top: -12px; left: 50%; transform: translateX(-50%); background: #000; color: #fff; padding: 2px 10px; font-size: 10px; border-radius: 10px; }\n</style>"
