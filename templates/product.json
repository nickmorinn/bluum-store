lue = maxInventory;\n            newClonedQtyField.value = maxInventory;\n          }\n          \n          qtyField.value = newValue;\n          qtyField.dispatchEvent(new Event('change', { bubbles: true }));\n          updateOfferPrices(newValue);\n          updateButtonStates(newClonedQtyField);\n        });\n        \n        updateButtonStates(newClonedQtyField);\n        updateOfferPrices(parseInt(newClonedQtyField.value));\n      }\n    } else {\n      quantitySelector.style.display = 'block';\n      const clonedSelector = document.querySelector('.zs-cloned-quantity');\n      if (clonedSelector) {\n        clonedSelector.remove();\n      }\n      \n      if (qtyField) {\n        qtyField.setAttribute('min', '1');\n        if (maxInventory !== null) {\n          qtyField.setAttribute('max', maxInventory);\n        }\n        updateButtonStates(qtyField);\n      }\n      \n      hideInventoryNotice();\n    }\n  }\n\n  function handleOfferBoxClick(e) {\n    const box = e.currentTarget;\n    \n    if (!isAvailable) {\n      return;\n    }\n    \n    if (box.classList.contains('zs-disabled')) {\n      showInventoryNotice(`Insufficient stock for this bundle option`, 'error');\n      return;\n    }\n    \n    elements.offerBoxes.forEach(b => b.classList.remove('zs-active'));\n    box.classList.add('zs-active');\n\n    const qty = box.dataset.qty;\n    \n    if (elements.qtyField) {\n      elements.qtyField.value = qty;\n      elements.qtyField.dispatchEvent(new Event('change', { bubbles: true }));\n      elements.qtyField.dispatchEvent(new Event('input', { bubbles: true }));\n    }\n\n    handleQuantityBox(qty);\n  }\n\n  function setupPriceObserver() {\n    const priceContainer = document.querySelector(SELECTORS.priceContainer);\n    if (!priceContainer) return;\n\n    if (priceObserver) {\n      priceObserver.disconnect();\n    }\n\n    priceObserver = new MutationObserver(() => {\n      setTimeout(() => {\n        const activeBox = document.querySelector('.zs-offer-box.zs-active');\n        const currentQty = activeBox?.dataset.qty === '3' ? parseInt(elements.qtyField?.value || 3) : null;\n        updateOfferPrices(currentQty);\n      }, 100);\n    });\n\n    priceObserver.observe(priceContainer, {\n      childList: true,\n      subtree: true,\n      characterData: true\n    });\n  }\n\n  async function initInventory() {\n    \n    isAvailable = await checkProductAvailability();\n    \n    if (!isAvailable) {\n      disableAllBundles();\n      return;\n    }\n    \n    enableAllBundles();\n    maxInventory = await getMaxInventory();\n    updateBundleAvailability();\n    \n    if (elements.qtyField && maxInventory !== null) {\n      elements.qtyField.setAttribute('max', maxInventory);\n    }\n    \n    const clonedQtyField = document.querySelector('.zs-cloned-quantity input[type=\"number\"]');\n    if (clonedQtyField && maxInventory !== null) {\n      clonedQtyField.setAttribute('max', maxInventory);\n      updateButtonStates(clonedQtyField);\n    }\n  }\n\n  async function init() {\n    cacheElements();\n\n    await initInventory();\n\n    if (elements.quantitySelector && isAvailable) {\n      elements.quantitySelector.style.display = 'none';\n    }\n\n    if (elements.variantPicker && elements.zsOffer) {\n      elements.variantPicker.insertAdjacentElement('afterend', elements.zsOffer);\n    }\n\n    setTimeout(() => {\n      updateDefaultPrice();\n      updateOfferPrices();\n    }, 300);\n\n    setupPriceObserver();\n\n    if (elements.zsOffer) {\n      elements.zsOffer.addEventListener('click', (e) => {\n        const box = e.target.closest(SELECTORS.offerBox);\n        if (box) handleOfferBoxClick.call(box, { currentTarget: box });\n      });\n    }\n\n    if (elements.qtyField) {\n      elements.qtyField.addEventListener('change', () => {\n        updateButtonStates(elements.qtyField);\n        const activeBox = document.querySelector('.zs-offer-box.zs-active');\n        if (activeBox?.dataset.qty === '3') {\n          updateOfferPrices(parseInt(elements.qtyField.value));\n        }\n      });\n      elements.qtyField.addEventListener('input', () => {\n        updateButtonStates(elements.qtyField);\n        const activeBox = document.querySelector('.zs-offer-box.zs-active');\n        if (activeBox?.dataset.qty === '3') {\n          updateOfferPrices(parseInt(elements.qtyField.value));\n        }\n      });\n    }\n\n    document.addEventListener('click', (e) => {\n      const qtyButton = e.target.closest('.quantity-selector__button');\n      if (!qtyButton) return;\n      \n      const isInCloned = qtyButton.closest('.zs-cloned-quantity');\n      if (!isInCloned) return;\n      \n      \n      e.preventDefault();\n      e.stopPropagation();\n      e.stopImmediatePropagation();\n      \n      const clonedSelector = qtyButton.closest('.zs-cloned-quantity');\n      const clonedQtyField = clonedSelector.querySelector('input[type=\"number\"]');\n      const originalQtyField = document.querySelector(SELECTORS.qtyField);\n      \n      if (!clonedQtyField || !originalQtyField) return false;\n      \n      const buttons = clonedSelector.querySelectorAll('.quantity-selector__button');\n      const isMinus = buttons[0] === qtyButton;\n      \n      const currentVal = parseInt(clonedQtyField.value) || 3;\n      const minVal = parseInt(clonedQtyField.getAttribute('min')) || 3;\n      const maxVal = maxInventory !== null ? maxInventory : 999;\n      \n      \n      let newVal = currentVal;\n      \n      if (isMinus) {\n        if (currentVal > minVal) {\n          newVal = currentVal - 1;\n          hideInventoryNotice();\n        } else {\n          return false;\n        }\n      } else {\n        if (currentVal < maxVal) {\n          newVal = currentVal + 1;\n        } else {\n          showInventoryNotice(`Only ${maxVal} item${maxVal !== 1 ? 's' : ''} available`, 'warning');\n          return false;\n        }\n      }\n      \n      clonedQtyField.value = newVal;\n      originalQtyField.value = newVal;\n      \n      originalQtyField.dispatchEvent(new Event('change', { bubbles: true }));\n      originalQtyField.dispatchEvent(new Event('input', { bubbles: true }));\n      \n      setTimeout(() => {\n        updateButtonStates(clonedQtyField);\n        updateOfferPrices(newVal);\n      }, 10);\n      \n      return false;\n    }, true);\n\n    document.addEventListener('change', async (e) => {\n      if (e.target.matches('select[name=\"id\"]') || e.target.matches('input[name=\"id\"]')) {\n        await initInventory();\n      }\n    });\n\n    setTimeout(() => {\n      const activeBox = document.querySelector('.zs-offer-box.zs-active');\n      if (elements.quantitySelector && activeBox && isAvailable) {\n        if (activeBox.dataset.qty === '3') {\n          elements.quantitySelector.style.display = 'block';\n        } else {\n          elements.quantitySelector.style.display = 'none';\n        }\n      }\n    }, 600);\n    \n  }\n\n  if (document.readyState === 'loading') {\n    document.addEventListener('DOMContentLoaded', init);\n  } else {\n    init();\n  }\n})();\n</script>"
