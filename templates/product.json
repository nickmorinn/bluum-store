tor__button, .quantity__button',\n    offerBox: '.zs-offer-box',\n    qtySelectorArea: '.product-info__quantity-selector, quantity-selector'\n  };\n\n  // --- 1. IMPROVED PRICE FETCHING ---\n  function getUnitPrice() {\n    try {\n      // Get current Variant ID\n      const currentId = document.querySelector(SELECTORS.variantInput)?.value;\n      if (!currentId) return null;\n\n      // Method A: Try Shopify's global product object (most reliable)\n      if (window.ShopifyAnalytics?.meta?.product?.variants) {\n        const v = window.ShopifyAnalytics.meta.product.variants.find(x => String(x.id) === String(currentId));\n        if (v) return v.price / 100;\n      }\n\n      // Method B: Parse the Web Pixels Manager script (your original method)\n      const script = document.getElementById('web-pixels-manager-setup');\n      if (script) {\n        const match = script.textContent.match(/\"productVariants\":\\s*(\\[.+?\\])\\s*,\"purchasingCompany\"/);\n        if (match && match[1]) {\n          const variants = JSON.parse(match[1]);\n          const v = variants.find(x => String(x.id) === String(currentId));\n          if (v) return parseFloat(v.price.amount);\n        }\n      }\n    } catch (e) { console.error('Price fetch error', e); }\n    return null;\n  }\n\n  // --- 2. UI UPDATE ---\n  function updateAllDisplays() {\n    const unitPrice = getUnitPrice();\n    if (!unitPrice) return;\n\n    document.querySelectorAll(SELECTORS.offerBox).forEach(box => {\n      const baseQty = parseInt(box.dataset.qty);\n      let calcQty = baseQty;\n\n      if (baseQty === 3 && box.classList.contains('zs-active')) {\n        const input = box.querySelector('.zs-cloned-quantity input') || document.querySelector(SELECTORS.mainQtyField);\n        calcQty = parseInt(input.value) || 3;\n        const title = box.querySelector('.zs-title');\n        if (title) title.textContent = `Buy ${calcQty}`;\n      } else if (baseQty === 3) {\n        const title = box.querySelector('.zs-title');\n        if (title) title.textContent = \"Buy 3 +\";\n      }\n\n      const discount = calcQty >= 3 ? 0.10 : (calcQty >= 2 ? 0.05 : 0);\n      const originalTotal = unitPrice * calcQty;\n      const finalTotal = originalTotal * (1 - discount);\n\n      if (baseQty === 1) {\n        const p = box.querySelector('.zs-price-default');\n        if (p) p.textContent = '$' + unitPrice.toFixed(2);\n      } else {\n        const s = box.querySelector('.zs-price');\n        const o = box.querySelector('.zs-old-price');\n        if (s) s.textContent = '$' + finalTotal.toFixed(2);\n        if (o) o.textContent = '$' + originalTotal.toFixed(2);\n      }\n    });\n  }\n\n  // --- 3. VARIANT CHANGE OBSERVER ---\n  // This watches the hidden ID input for any changes made by the theme's JS\n  function initVariantObserver() {\n    const targetNode = document.querySelector(SELECTORS.variantInput);\n    if (!targetNode) return;\n\n    const observer = new MutationObserver(() => {\n      updateAllDisplays();\n    });\n\n    // Watch for value attribute changes\n    observer.observe(targetNode, { attributes: true, childList: true, characterData: true });\n    \n    // Also listen for standard change events just in case\n    targetNode.addEventListener('change', updateAllDisplays);\n  }\n\n  // --- 4. QUANTITY LOGIC ---\n  function toggleQuantityClones(qty) {\n    const originalArea = document.querySelector(SELECTORS.qtySelectorArea);\n    if (!originalArea) return;\n\n    if (qty === '3') {\n      const box3 = document.querySelector('.zs-offer-box[data-qty=\"3\"]');\n      let cloned = box3.querySelector('.zs-cloned-quantity');\n      if (!cloned) {\n        cloned = originalArea.cloneNode(true);\n        cloned.classList.add('zs-cloned-quantity');\n        box3.appendChild(cloned);\n        const input = cloned.querySelector('input');\n        input.setAttribute('min', '3');\n        input.value = '3';\n        input.addEventListener('input', () => {\n          if (parseInt(input.value) < 3) input.value = 3;\n          syncMainQuantity(input.value);\n        });\n      }\n      originalArea.style.display = 'none';\n    } else {\n      originalArea.style.display = 'block';\n      const cloned = document.querySelector('.zs-cloned-quantity');\n      if (cloned) cloned.remove();\n    }\n  }\n\n  function syncMainQuantity(val) {\n    const main = document.querySelector(SELECTORS.mainQtyField);\n    if (main) {\n      main.value = val;\n      main.dispatchEvent(new Event('change', { bubbles: true }));\n      updateAllDisplays();\n    }\n  }\n\n  function init() {\n    // Box Clicks\n    document.querySelectorAll(SELECTORS.offerBox).forEach(box => {\n      box.addEventListener('click', function() {\n        document.querySelectorAll(SELECTORS.offerBox).forEach(b => b.classList.remove('zs-active'));\n        this.classList.add('zs-active');\n        const qty = this.dataset.qty;\n        syncMainQuantity(qty);\n        toggleQuantityClones(qty);\n      });\n    });\n\n    // Cloned button clicks\n    document.addEventListener('click', (e) => {\n      const btn = e.target.closest(SELECTORS.qtyButton);\n      if (!btn || !btn.closest('.zs-cloned-quantity')) return;\n      e.preventDefault();\n      const input = btn.closest('.zs-cloned-quantity').querySelector('input');\n      let currentVal = parseInt(input.value) || 3;\n      const isMinus = btn.textContent.includes('-') || btn.classList.contains('minus') || btn === btn.parentNode.firstElementChild;\n      if (isMinus) { if (currentVal > 3) currentVal--; } else { currentVal++; }\n      input.value = currentVal;\n      syncMainQuantity(currentVal);\n    }, true);\n\n    initVariantObserver();\n    updateAllDisplays();\n    \n    // Fallback: check every second for price changes (in case of weird theme JS)\n    setInterval(updateAllDisplays, 2000);\n  }\n\n  window.addEventListener('load', init);\n})();\n</script>\n\n<style>\n  .zs-offer-wrapper { \n    display: grid;\n    grid-template-columns: repeat