function() {\n  'use strict';\n  \n  const SELECTORS = {\n    variantPicker: '.variant-picker__option',\n    quantitySelector: '.product-info__quantity-selector, quantity-selector',\n    qtyField: 'input[type=\"number\"][name=\"quantity\"]',\n    qtyButton: '.quantity-selector__button, .quantity__button',\n    zsOffer: '.zs-offer-wrappers',\n    defaultPrice: '.zs-price-default',\n    offerBox: '.zs-offer-box',\n    inventoryNotice: '.zs-inventory-notice'\n  };\n\n  let elements = {};\n  let maxInventory = null;\n  let isAvailable = true;\n  let cachedVariants = null;\n\n  // --- 1. CORE PRICE DETECTION (PIXEL SCRIPT PARSING) ---\n  function getMerchandiseData() {\n    if (cachedVariants) return cachedVariants;\n    try {\n      const script = document.getElementById('web-pixels-manager-setup');\n      if (!script) return null;\n      const match = script.textContent.match(/\"productVariants\":\\s*(\\[.+?\\])\\s*,\"purchasingCompany\"/);\n      if (match && match[1]) {\n        cachedVariants = JSON.parse(match[1]);\n        return cachedVariants;\n      }\n    } catch (e) { console.error('[ZS] Data Parse Error:', e); }\n    return null;\n  }\n\n  function getActiveUnitPrice() {\n    const variants = getMerchandiseData();\n    if (!variants) return null;\n    let currentId = document.querySelector('input[name=\"id\"], select[name=\"id\"]')?.value;\n    const match = variants.find(v => String(v.id) === String(currentId));\n    return match ? parseFloat(match.price.amount) : parseFloat(variants[0].price.amount);\n  }\n\n  // --- 2. INVENTORY & AVAILABILITY API ---\n  async function fetchVariantAvailability(variantId) {\n    try {\n      const response = await fetch(`${window.location.pathname}.js`);\n      if (!response.ok) return null;\n      const productData = await response.json();\n      return productData.variants.find(v => String(v.id) === String(variantId));\n    } catch (error) { return null; }\n  }\n\n  async function initInventory() {\n    const variantId =